<!DOCTYPE html>
<html lang="en">

    <head>
        <% include layouts/head %>
    </head>

    <body>

        <div id="wrapper">
            <% include layouts/header %>

            <!-- Page Content -->
            <div id="page-content-wrapper" ng-app="dashboard">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <h1>Dashboard</h1>
                            <div class="row">
                                <div class="col-lg-6 col-sm-12" ng-controller="stockReoportCtrl">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">Today's Stock</div>
                                        <div class="panel-body">
                                            <canvas id="pie" height="200" width="350" class="chart chart-pie" chart-data="data" chart-labels="labels" chart-options="options">
                                            </canvas>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /#page-content-wrapper -->

        </div>
        <!-- /#wrapper -->

        <% include layouts/foot %>
        <script src="/bower_components/chart.js/dist/Chart.min.js"></script>
        <script src="/bower_components/angular-chart.js/dist/angular-chart.min.js"></script>
        <script src="/javascripts/dashboard.js"></script>
    </body>

</html>

